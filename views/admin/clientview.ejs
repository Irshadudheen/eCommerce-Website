      <div class="container mt-5">
        <div class="row tm-content-row">
          <div class="col-12 tm-block-col">
            <div class="tm-bg-primary-dark tm-block tm-block-h-auto">
              <h2 class="tm-block-title">List of Users</h2>
              
              <!-- Search Bar -->
              <div class="input-group mb-3">
                <input type="text" class="form-control" placeholder="Search for users..." oninput="searchUsers(this.value)">
              </div>
              <input type="hidden" name="data" id="data" value="<%=data%>">
              <table class="table table-striped">
                <thead>
                  <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>email</th>
                    <th>Actions</th>
                  </tr>
                </thead>
                <tbody id="userTableBody">
                  <!-- Example user data, replace this with your actual user data -->
                  <% 
                  if(data.length >0){
                      for(let i=0;i<data.length;i++){
                          %>
                  <tr>

                    <td><%=i+1%></td>
                    <td><%=data[i].fname%></td>
                    <td><%=data[i].email%></td>
                    <td>
                     
                     <a href="/admin/editUser?id=<%=data[i]._id%>"><button class="btn btn-info btn-sm" >Edit</button></a>
                     <form style="display:inline-block ;" action="/admin/deleteClient" method="post"> <button onclick="delet()" name="delet" id="delete" value="<%=data[i]._id%>" class="btn btn-danger btn-sm" >Delete</button></form>
                     <a href="/admin/blockClient?id=<%=data[i]._id%>"><button class="btn btn-warning btn-sm" >Block</button></a>
                   </td>
                  </tr>
                  <%
            }
        }else{
            %>
            <tr>
                <td colspan="5">users not found</td>
                </tr>
                <%
            }
            %>
                  </tbody>
                
                </table>

              </table>
            </div>
          </div>
        </div>
