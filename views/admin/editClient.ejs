      <div class="container mt-5">
        <div class="row tm-content-row">
          <div class="col-12 tm-block-col">

          </div>
        </div>
        <!-- row -->

        <div class="tm-block-col tm-col-account-settings">
          <div class="tm-bg-primary-dark tm-block tm-block-settings">
            <h2 class="tm-block-title">Account Settings</h2>
            

            <form action="/admin/updateClient" method="post" class="tm-signup-form row">
              <div class="form-group col-lg-6">
                <label for="name">Account Name</label>
                <input id="name" name="name" type="text" onkeyup="nameCheck(this)"
                <%  if( typeof client !=="undefined"){%>
                 value="<%=client.fname%>" 
                 <%}%>
                 class="form-control validate" />
                 <script>
                  function nameCheck(tag){
                    const input = tag.value.trim()
                    const submit = document.getElementById('submit')
                    if(input.length>0){
                      submit.disabled=false
                    }else{
                      submit.disabled=true
                    }

                  }
                 </script>
              </div>
              <div class="form-group col-lg-6">
                <label for="email">Account Email</label>
                <input id="email" name="email" type="email" onkeyup="emailCheck(this)"
                <%  if( typeof client !=="undefined"){%>
                 value="<%=client.email%>"
                 <%}%>
                  class="form-control validate" />
                  <script>
                    function emailCheck(tag){
                      const input = tag.value.trim()
                      const submit = document.getElementById('submit')
                      if(input.length>0){
                        submit.disabled=false
                      }else{
                        submit.disabled=true
                      }
  
                    }
                   </script>
              </div>


              <div class="form-group col-lg-6">
                <label for="phone">Phone</label>
                <input id="phone" name="phone"  min="1" type="number" 
                
                <%  if( typeof client !=="undefined"){%>
                value="<%=client.mobile%>"

                <%}%>
                class="form-control validate" />
                
              </div>
              <div class="form-group col-lg-6">
              <%  if( typeof message !=="undefined"){%>
                <h6><%=message%></h6>
                <%}%>

              </div>
              <div class="form-group col-lg-6">
                <label class="tm-hide-sm">&nbsp;</label>

              </div>
              <div class="col-12">
                <input type="hidden" 
                value="<%=id%>" name="id">
                <input type="hidden" value="<%=client%>">
                <button type="submit" id="submit" class="btn btn-primary btn-block text-uppercase">
                  update your profile
                </button>
              </div>
            </form>
          </div>
        </div>
      </div>
