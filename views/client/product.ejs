    <script>
function searchProduct() {
    var input, filter, products, div, h3, a, i, txtValue;

    input = document.getElementById("myInput");
    if (!input) {
        console.error("Input element with id 'myInput' not found");
        return; 
    }

    filter = input.value.toUpperCase();

    products = document.getElementById("rowData");
    if (!products) {
        console.error("Products container with id 'rowData' not found");
        return; 
    }

    div = products.getElementsByClassName('product'); 

    for (i = 0; i < div.length; i++) {
        h3 = div[i].getElementsByTagName("h3")[0];
        if (h3) {
            a = h3.getElementsByTagName("a")[0];
            if (a) {
                txtValue = a.textContent || a.innerText;
                if (txtValue.toUpperCase().indexOf(filter) > -1) {
                    div[i].style.display = ""; 
                } else {
                    div[i].style.display = "none"; 
                }
            }
        }
    }
}


    </script>
